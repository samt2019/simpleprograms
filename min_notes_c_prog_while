#include<stdio.h>

int main()
{
    int i, j=0;
    scanf("%d", &i);
    while(i>=100){
        i-=100;
        j+=1;
    }
    while(i>=50){
        i-=50;
        j+=1;
    }
    while(i>=10){
        i-=10;
        j+=1;
    }
    while(i>=5){
        i-=5;
        j+=1;
    }
    while(i>=2){
        i-=2;
        j+=1;
    }
    while(i>=1){
        i-=1;
        j+=1;
    }
    printf("%d", j);
    return 0;
}
